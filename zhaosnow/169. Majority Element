//最简单的O（n）算法，利用两两相消的做法。
class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int count=0,tmp;
        for(int i=0;i<nums.size();i++){
            if(count==0){
                tmp=nums[i];
                count++;
            }else if(tmp==nums[i]){
                count++;
            }else count--;
        }
         return tmp;
    }
};
